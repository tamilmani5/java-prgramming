class Main
{
public static void main(String[] args){
       int[][]arr=new int [3][3];
       int num=0;
       for(int i=0;i<3;i++){
           for(int j=0;j<3;j++){
               arr[i][j]=num;
               num++;
               System.out.print(num+"  ");
           }
           System.out.println();
        }
}
}
