import java.util.Scanner;
class Main {
    public static void main(String[] args) {
       
        Scanner sun=new Scanner(System.in);
       
        System.out.println("ENTER THE N VALUE:");
        int n=sun.nextInt();
        {
            for(int i=1;i<=n;i++)
            {
                for(int j=1;j<=i;j++)
                System.out.print(i*j);
           
            System.out.println();
            }
        }
    }
}
