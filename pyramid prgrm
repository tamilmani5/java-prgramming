import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);  
      int num = 1;
      int n = 6;
        // System.out.println("Enter the no. of seats");
        // int nx = sc.nextInt();
            for(int i=1;i<=n;i++)
            {
                for(int j=1;j<=i;j++){
                     System.out.print(num+" ");
                     num++;
                }
               System.out.println();
            }
           
       
    }
}
