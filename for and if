class Main {
    public static void main(String[] args) {
        int i;
        String[]Array={"aaa","bbb","ccc"};
        for(i=0;i<Array.length;i++){
        System.out.println(Array[i]);
        if(Array[i]=="ccc"){
            Array[]="ddd";
        System.out.println( Array[i]);
        }
    }
}
}
